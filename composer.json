{
    "name": "ze-pequeno/pequeno-spotify-module",
    "description": "Zend Framework 2 Module for Spotify Web API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "service",
        "spotify",
        "module",
        "zf2"
    ],
    "homepage": "https://github.com/ze-pequeno/pequeno-spotify-module",
    "authors": [
        {
            "name": "Cyril Martin",
            "email": "cyril.martin.cm@gmail.com",
            "homepage": "http://ze-pequeno.github.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ze-pequeno/pequeno-spotify-service"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ze-pequeno/pequeno-spotify-service": "*@dev"
    },
	"require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "satooshi/php-coveralls": "*@dev",
        "zendframework/zendframework": "~2.3",
        "phpunit/phpunit": "~4.0"
	},
    "autoload": {
        "psr-0": {
            "PequenoSpotifyModule\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "2.0-dev"
        }
    }
}